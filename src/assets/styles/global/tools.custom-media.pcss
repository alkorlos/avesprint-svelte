/* Viewport/Page Dimensions */
@custom-media --desktop-large (width > 1920px);

@custom-media --desktop (width <= 1920px);

@custom-media --desktop-1300 (width <= 1300px);

@custom-media --desktop-1200 (width <= 1200px);

@custom-media --desktop-1100 (width <= 1100px);

@custom-media --tablet (width <= 992px);

@custom-media --phone (width <= 768px);

@custom-media --phone-576 (width <= 576px);

/* Interaction */
@custom-media --hover-fine (hover: hover), (pointer: fine);
